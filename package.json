{
    "name": "svelte-typescript-esbuild-bootstrap5",
    "version": "1.0.0",
    "description": "Svelte + TypeScript + ESBuild + Bootstrap 5 + LiveReload Template",
    "type": "module",
    "scripts": {
        "build": "node esbuild.config.js prod",
        "watch": "node esbuild.config.js dev",
        "serve": "serve dist -p 8000",
        "dev": "concurrently \"npm run watch\" \"npm run serve\" \"npm run livereload\"",
        "livereload": "livereload dist/",
        "validate": "svelte-check"
    },
    "keywords": [
        "esbuild",
        "svelte",
        "bootstrap",
        "bootstrap 5",
        "livereload",
        "typescript"
    ],
    "author": "ebolax",
    "license": "ISC",
    "devDependencies": {
        "@pyoner/svelte-types": "^3.4.4-2",
        "@tsconfig/svelte": "^2.0.1",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "bootstrap": "^5.1.1",
        "concurrently": "^6.2.1",
        "esbuild": "^0.12.28",
        "esbuild-svelte": "^0.5.6",
        "eslint": "^7.32.0",
        "eslint-plugin-svelte3": "^3.2.1",
        "livereload": "^0.9.3",
        "serve": "^12.0.1",
        "svelte": "^3.42.6",
        "svelte-check": "^2.2.6",
        "svelte-preprocess": "^4.9.5",
        "typescript": "^4.4.3"
    }
}
